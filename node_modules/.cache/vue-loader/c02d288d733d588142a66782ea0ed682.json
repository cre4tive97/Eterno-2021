gin-top: 10px;\n  padding: 8px 16px;\n  border-style: none;\n  background: rgba(255, 255, 255, 0.3);\n  border-radius: 8px;\n  font-size: 1rem;\n  color: rgba(255, 255, 255, 0.8);\n  transition: all 0.5s;\n}\n.intro-login__button:hover {\n  cursor: pointer;\n  background: rgba(255, 255, 255, 0.5);\n}\n.loginFadeIn {\n  opacity: 1;\n}\n\n.loginFadeOut {\n  opacity: 0;\n}\n</style>\n"]}]}