 }\n    },\n    clickDislike() {\n      if (this.instagramData[this.i].liked === true) {\n        this.$emit(\"dislike\");\n      }\n    },\n  },\n};\n</script>\n\n<style>\n.instagram__content {\n  color: white;\n  width: 100%;\n  margin-bottom: 2rem;\n}\n.instagram__content__profile {\n  margin: 0 1.5rem;\n  display: flex;\n  align-items: center;\n  margin-bottom: 0.5rem;\n}\n.instagram__content__profile__image {\n  width: 30px;\n  height: 30px;\n  background-size: 100%;\n  border-radius: 50%;\n  margin-right: 0.5rem;\n}\n.instagram__content__post__image {\n  width: 100%;\n  height: 300px;\n  background-size: cover;\n  margin-bottom: 0.3rem;\n}\n.instagram__content__post__btnGroup {\n  margin: 0 1.5rem;\n  font-size: 1.25rem;\n  margin-bottom: 4px;\n}\n.instagram__content__post__btnGroup i {\n  margin-right: 1rem;\n}\n.instagram__content__post__like {\n  margin: 0 1.5rem;\n}\n.instagram__content__post__text {\n  margin: 0 1.5rem;\n  display: flex;\n  align-items: center;\n  font-size: 1rem;\n}\n.instagram__content__post__username {\n  margin-right: 0.5rem;\n}\n</style>\n"]}]}